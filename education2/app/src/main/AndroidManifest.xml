                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".UI.LoginActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.TeachPlanActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.QueryGradeActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.LevelExamActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.ClassTableActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.TeachNitificationActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.WebviewActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.NewsFragment.LigongFengcaiActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.NewsFragment.LigongWenyuanActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.NewsFragment.SchoolNewsActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.NewsFragment.StudentActivityActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.NewsFragment.TeachStudyActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UI.NewsFragment.UniversityCultureActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>